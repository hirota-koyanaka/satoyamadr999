{
  "buildCommand": "cd login_app && flutter build web --release",
  "outputDirectory": "login_app/build/web",
  "devCommand": "cd login_app && flutter run -d chrome",
  "installCommand": "cd login_app && flutter pub get",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

